{{ form_start(form) }}

<div class="my-custom-class-for-errors">
    {{ form_errors(form) }}
</div>

<div class="row">
    <div class="col-md-6" id="some-custom-id">
        {{ form_row(form.message) }}
    </div>

    <div class="col-md-2">
        {{ form_row(form.name) }}
        {{ form_row(form.phone) }}
        {{ form_row(form.a_choice) }}
        {{ form_row(form.save) }}
    </div>

</div>

{{ form_end(form) }}